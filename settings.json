{
  "workbench.iconTheme": "vscode-icons",
  "editor.fontFamily": "Consolas, 'Courier New', monospace",
  "workbench.editorAssociations": {
    "*.copilotmd": "vscode.markdown.preview.editor",
    "{hexdiff}:/**/*.*": "hexEditor.hexedit",
    "*.md": "default",
    "*.wasm": "hexEditor.hexedit",
    "{git,gitlens,git-graph}:/**/*.{md,csv,svg}": "default"
  },
  "window.newWindowDimensions": "inherit",
  "editor.fontSize": 13,
  "editor.guides.bracketPairs": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.formatOnPaste": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.inlineSuggest.enabled": true,
  "editor.largeFileOptimizations": false,
  "git.confirmSync": false,
  "git.suggestSmartCommit": false,
  "git.autofetch": "all",
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": true
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.chat.edits.codesearch.enabled": true,
  "gitlens.codeLens.enabled": false,
  "discord.lowerDetailsDebugging": "{file_name} {current_line}/{total_lines} 行",
  "discord.detailsEditing": "編集中",
  "discord.detailsIdling": "待機中",
  "discord.largeImage": "{LANG} を編集中",
  "discord.largeImageIdling": "待機中",
  "discord.lowerDetailsEditing": "{file_name} {current_line}/{total_lines} 行",
  "discord.lowerDetailsIdling": "待機中",
  "discord.detailsDebugging": "デバッグ中",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "vsicons.dontShowNewVersionMessage": true,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "diffEditor.wordWrap": "off",
  "remote.SSH.useLocalServer": true,
  "remote.SSH.enableRemoteCommand": true,
  "remote.SSH.suppressWindowsSshWarning": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "files.exclude": {
    "**/*.g.dart": true,
    "**/*.freezed.dart": true,
    "**/*.gr.dart": true,
    "**/*.mocks.dart": true,
    "__pycache__": true
  }
}
