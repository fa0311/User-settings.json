{
  "editor.fontSize": 13,
  "workbench.iconTheme": "vscode-icons",
  "window.newWindowDimensions": "inherit",
  "editor.guides.bracketPairs": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "git.confirmSync": false,
  "editor.largeFileOptimizations": false,
  "discord.lowerDetailsDebugging": "{file_name} {current_line}/{total_lines} 行",
  "discord.detailsEditing": "編集中",
  "discord.detailsIdling": "待機中",
  "discord.largeImage": "{LANG} を編集中",
  "discord.largeImageIdling": "待機中",
  "discord.lowerDetailsEditing": "{file_name} {current_line}/{total_lines} 行",
  "discord.lowerDetailsIdling": "待機中",
  "discord.detailsDebugging": "デバッグ中",
  "discord.removeRemoteRepository": true,
  "trailing-spaces.backgroundColor": "rgba(100,100,100,0.15)",
  "trailing-spaces.borderColor": "rgba(255,255,255,0.3)",
  "remote.SSH.remotePlatform": {
    "192.168.11.20": "linux",
    "192.168.11.18": "windows"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[php]": {
    "editor.defaultFormatter": null
  },
  "[dart]": {
    "editor.rulers": [80],
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "redhat.telemetry.enabled": false,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "cSpell.enabled": false,
  "githubPullRequests.pullBranch": "never",
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": true,
  "dart.showInspectorNotificationsForWidgetErrors": false,
  "vsicons.dontShowNewVersionMessage": true
}
